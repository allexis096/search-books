{
  "preset": "react-native",
  "setupFilesAfterEnv": ["@testing-library/jest-native/extend-expect", "<rootDir>/.jest/setup.ts"],
  "testPathIgnorePatterns": ["/node_modules/"],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.ts(x)?"
  ],
  "modulePaths": ["<rootDir>/src/", "<rootDir>/.jest"]
}